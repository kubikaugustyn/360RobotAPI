@COOKIE = q=u=&t=1;t=&v=2.0&a=1; qid={{QID}}; sid={{SID}}

### Changes the clean mode / suction power (quiet/auto/strong/max)
# The modes are called Quiet/Standard/Powerful/MAX in the UI
# cleanType can be total/area
@CLEAN_MODE = auto
POST https://q.smart.360.cn/clean/cmd/send
Content-Type: application/x-www-form-urlencoded
Cookie: {{COOKIE}}

sn = {{SN}} &
infoType = 21022 &
data = {"cmd": "{{CLEAN_MODE}}", "cleanType": "total"} &
devType = 3
